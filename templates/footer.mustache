{{!
    @template theme_adaptable/footer

    Footer.

    Example context (json):
    {
    }
}}
{{{output.standard_after_main_region_html}}}
<footer id="page-footer" class="{{responsivepagefooter}}">
    <div id="course-footer">{{{output.course_footer}}}</div>
    {{#showfooterblocks}}{{{output.get_footer_blocks}}}{{/showfooterblocks}}
    {{#socialicons}}
    <div class="container">
        <div class="row">
            <div class="col-12 pagination-centered socialicons">
            {{{socialicons}}}
            </div>
        </div>
    </div>
    {{/socialicons}}
    <div class="info container2 clearfix">
        <div class="container">
            <div class="row">
                <div class="tool_usertours-resettourcontainer"></div>
                {{#footnote}}
                <div class="{{#moodledocs}}col-md-4{{/moodledocs}}{{^moodledocs}}col-md-8{{/moodledocs}} my-md-0 my-2">{{{footnote}}}</div>
                {{/footnote}}
                {{#moodledocs}}
                <div class="col-md-4 my-md-0 my-2 helplink">{{{output.page_doc_link}}}</div>
                {{/moodledocs}}
                <div class="col-md-4 my-md-0 my-2">{{{output.standard_footer_html}}}</div>
            </div>
            {{#debug}}
            <div class="row">
                <div class="col-12 my-md-0 my-2">
                    {{{debug}}}
                </div>
            </div>
            {{/debug}}
        </div>
    </div>
</footer>
<div id="back-to-top"><i class="fa fa-angle-up "></i></div>
{{#savediscard}}{{{savediscard}}}{{/savediscard}}

</div>{{! End #page. }}
</div>{{! End #page-wrapper. }}

{{{output.standard_end_of_body_html}}}
